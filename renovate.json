{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    ":automergePatch",
  ],
  "enabledManagers": ["npm"],
  "rangeStrategy": "update-lockfile",
  "packageRules": [
    {
      "packagePatterns": ["*"],
      "managers": ["npm"],
      "patch": {
        "groupName": "all js packages",
        "groupSlug": "all-js"
      }
    }
  ]
}
